<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kesavan.studios.kesav.pushpull">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic"
        android:label="Push Pull Legs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="kesavan.studios.kesav.pushpull.SplashScreen"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
        <activity
            android:name="kesavan.studios.kesav.pushpull.signin"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="kesavan.studios.kesav.pushpull.register"
            android:configChanges="orientation"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="kesavan.studios.kesav.pushpull.MainA"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>